{
  "name": "my-blog",
  "version": "0.0.1",
  "description": "elvinn blog",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elvinn/my-blog.git"
  },
  "author": "elvinn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elvinn/my-blog/issues"
  },
  "homepage": "https://github.com/elvinn/my-blog#readme",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "_moduleAliases": {
    "@consts": "dist/consts",
    "@controllers": "dist/controllers",
    "@models": "dist/models",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.11",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.1.6",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^24.9.0",
    "kill-port": "^1.6.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  }
}